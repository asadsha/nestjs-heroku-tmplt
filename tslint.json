{
    "defaultSeverity": "error",
    "extends": [],
    "jsRules": {
    },
    "rules": {
        "trailing-comma": false,
        "quotemark": [
            true,
            "single"
        ],
        "ordered-imports": [
            false
        ],
        "max-line-length": [
            true,
            150
        ],
        "interface-name": [
            false
        ],
        "array-type": [
            false
        ],
        "object-literal-sort-keys": false,
        "curly": false,
        "one-line": [
            false
        ],
        "one-variable-per-declaration": [
            false
        ]
    },
    "rulesDirectory": []
}
